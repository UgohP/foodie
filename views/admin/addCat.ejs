<main>
    <form action="/addCat" method="POST" enctype="multipart/form-data">
        <div class="head-title">
            <div class="left">
                <h1>Add Category</h1>
            </div>
        </div>
        <label for="name">Name</label>
        <input type="text" id="name" name="name" placeholder="Item Name">

        <label for="image">Image</label>
        <input type="file" id="image" name="image" placeholder="upload Image">

        <input type="submit" value="Submit">
    </form>
</main>